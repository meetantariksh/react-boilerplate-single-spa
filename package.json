{
    "name": "@org-name/prj-name",
    "engines": {
        "npm": ">=5",
        "node": ">=8.15.1"
    },
    "author": "Antariksh Roy",
    "license": "MIT",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.config.dev.js",
        "start:production": "npm run test && npm run build && npm run start:prod",
        "start:standalone": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.config.dev.js --env standalone",
        "start:prod": "cross-env NODE_ENV=production webpack serve --config webpack/webpack.config.js",
        "analyze": "node ./internals/scripts/analyze.js",
        "extract-intl": "node ./internals/scripts/extract-intl.js",
        "prebuild": "npm run build:clean",
        "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js --color -p --progress --hide-modules --display-optimization-bailout",
        "build:clean": "rimraf ./build",
        "format": "prettier --write .",
        "check-format": "prettier --check .",
        "watch-tests": "cross-env BABEL_ENV=test jest --watch",
        "coverage": "cross-env BABEL_ENV=test jest --coverage",
        "generate": "plop --plopfile internals/generators/index.js",
        "lint": "npm run lint:js",
        "lint:css": "stylelint src/**/*.js",
        "lint:eslint": "eslint src --ext js",
        "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
        "lint:js": "npm run lint:eslint",
        "lint:staged": "lint-staged",
        "pretest": "npm run test:clean && npm run lint",
        "test:clean": "rimraf ./coverage",
        "test": "cross-env NODE_ENV=test jest --coverage",
        "test:watch": "cross-env NODE_ENV=test jest --watchAll",
        "coveralls": "cat ./coverage/lcov.info | coveralls",
        "prettify": "prettier --write"
    },
    "eslintConfig": {
        "extends": [
            "react-important-stuff"
        ]
    },
    "lint-staged": {
        "*.js": [
            "npm run lint:eslint:fix",
            "git add --force"
        ],
        "*.json": [
            "prettier --write",
            "git add --force"
        ]
    },
    "pre-commit": "lint:staged",
    "resolutions": {
        "babel-core": "7.0.0-bridge.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.16",
        "@babel/eslint-parser": "^7.12.16",
